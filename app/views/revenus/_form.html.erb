<% form_for( [ @foyer, @revenu ] ) do |f| %>
  <%= f.error_messages %>
  <fieldset>
    
    <legend>Revenu du foyer</legend>

    <%= f.label 'Type:' %>
    <%= f.enum_select :type_de_revenu %><br />

    <%= f.label :valeur %>
    <%= f.text_field :valeur %><br />
    
  </fieldset>
  <br />
  <%= f.submit 'Ok' %>
<% end %>